    "@waves/ts-lib-crypto": "^1.4.4-beta.1",
    "@waves/waves-transactions": "^4.2.10",
    "c8": "^7.9.0",
    "chai": "^4.3.6",
    "chromedriver": "^104.0.0",
    "codecov": "^3.8.3",
    "eslint": "8.22",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.1",
    "mocha": "^9.1.1",
    "parse-json-bignumber": "^0.1.3",
    "prettier": "^2.7.1",
    "selenium-webdriver": "^4.0.0-rc-1",
