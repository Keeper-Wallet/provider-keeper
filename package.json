    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wavesplatform/provider-keeper"
  },
  "bugs": {
    "url": "https://github.com/wavesplatform/provider-keeper/issues",
    "email": "support@waves.tech"
  },
  "scripts": {
    "build": "vite build && vite build -c vite.config.min.ts && tsc",
    "watch": "vite build -c vite.config.min.ts",
    "start": "http-server .",
    "test": "mocha --exit",
    "coverage": "c8 yarn run test",
    "eslint": "eslint --ext .ts,.js --max-warnings 0 .",
    "prettier": "prettier . --check",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "mocha": {
