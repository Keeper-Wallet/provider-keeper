    "provider",
    "waveskeeper",
    "blockchain"
  ],
  "author": "smelnikov@web3tech.ru",
  "main": "dist/provider-keeper.cjs.js",
  "unpkg": "dist/provider-keeper.umd.js",
  "types": "dist/types/index.d.ts",
